<ul class="social-links inline-list">
  {% if settings.social_twitter != blank %}
  <li>
    <a class="icon-fallback-text" href="{{ settings.social_twitter }}" title="{{ 'layout.footer.store_on_social_network' | t: store_name: shop.name, social_network: 'Twitter' | escape }}">
      <span class="icon icon-twitter" aria-hidden="true"></span>
      <span class="fallback-text">Twitter</span>
    </a>
  </li>
  {% endif %}
  {% if settings.social_facebook != blank %}
  <li>
    <a class="icon-fallback-text" href="{{ settings.social_facebook }}" title="{{ 'layout.footer.store_on_social_network' | t: store_name: shop.name, social_network: 'Facebook' | escape }}">
      <span class="icon icon-facebook" aria-hidden="true"></span>
      <span class="fallback-text">Facebook</span>
    </a>
  </li>
  {% endif %}
  {% if settings.social_tumblr != blank %}
  <li>
    <a class="icon-fallback-text" href="{{ settings.social_tumblr }}" title="{{ 'layout.footer.store_on_social_network' | t: store_name: shop.name, social_network: 'Tumblr' | escape }}">
      <span class="icon icon-tumblr" aria-hidden="true"></span>
      <span class="fallback-text">Tumblr</span>
    </a>
  </li>
  {% endif %}
  {% if settings.social_instagram != blank %}
  <li>
    <a class="icon-fallback-text" href="{{ settings.social_instagram }}" title="{{ 'layout.footer.store_on_social_network' | t: store_name: shop.name, social_network: 'Instagram' | escape }}">
      <span class="icon icon-instagram" aria-hidden="true"></span>
      <span class="fallback-text">Instagram</span>
    </a>
  </li>
  {% endif %}
  {% if settings.social_pinterest != blank %}
  <li>
    <a class="icon-fallback-text" href="{{ settings.social_pinterest }}" title="{{ 'layout.footer.store_on_social_network' | t: store_name: shop.name, social_network: 'Pinterest' | escape }}">
      <span class="icon icon-pinterest" aria-hidden="true"></span>
      <span class="fallback-text">Pinterest</span>
    </a>
  </li>
  {% endif %}
  {% if settings.social_google_plus != blank %}
  <li>
    <a class="icon-fallback-text" href="{{ settings.social_google_plus }}" title="{{ 'layout.footer.store_on_social_network' | t: store_name: shop.name, social_network: 'Google+' | escape }}" rel="publisher">
      <span class="icon icon-google" aria-hidden="true"></span>
      <span class="fallback-text">Google Plus</span>
    </a>
  </li>
  {% endif %}
  {% if settings.social_youtube != blank %}
  <li>
    <a class="icon-fallback-text" href="{{ settings.social_youtube }}" title="{{ 'layout.footer.store_on_social_network' | t: store_name: shop.name, social_network: 'YouTube' | escape }}">
      <span class="icon icon-youtube" aria-hidden="true"></span>
      <span class="fallback-text">YouTube</span>
    </a>
  </li>
  {% endif %}
  {% if settings.social_vimeo != blank %}
  <li>
    <a class="icon-fallback-text" href="{{ settings.social_vimeo }}" title="{{ 'layout.footer.store_on_social_network' | t: store_name: shop.name, social_network: 'Vimeo' | escape }}">
      <span class="icon icon-vimeo" aria-hidden="true"></span>
      <span class="fallback-text">Vimeo</span>
    </a>
  </li>
  {% endif %}
  {% if settings.social_rss != blank %}
  <li>
    <a class="icon-fallback-text" href="/blogs/{{ settings.social_rss }}.atom" title="{{ shop.name }} RSS">
      <span class="icon icon-rss" aria-hidden="true"></span>
      <span class="fallback-text">RSS</span>
    </a>
  </li>
  {% endif %}
</ul>